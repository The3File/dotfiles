#!/usr/bin/env bash

# ============= #
# == AWRSTRT == #
# ============= #

setxkbmap -layout dk -variant nodeadkeys -option caps:escape &

runbar &
qutewal &

ln -sf    "${HOME}/.cache/wal/dunstrc"    "${HOME}/.config/dunst/dunstrc"
# Compton
pkill compton
compton --dbus -b &

# Dunst
pkill dunst
dunst &
