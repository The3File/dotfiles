#!/usr/bin/env bash

cp -r ~/.config/{dunst,i3,nvim,qutebrowser,ranger,wal,compton.conf} ~/dotfiles/.config/
cp -r ~/.Scripts ~/dotfiles/
cp -r ~/{.latexmkrc,.nvimrc,.profile,.vimrc,.X.old,.xinitrc,.Xmodmap,.Xresources,.xsessionrc,.zprofile,.zsh_aliases,.zshrc} ~/dotfiles/
cd ~/dotfiles/
git add .
git commit -a -m "gitdot"
git push -u origin master
